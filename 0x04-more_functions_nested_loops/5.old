#include "main.h"

/**
 *  more_numbers - print number 0 to 14 ten times
 *  start: starting value of loop
 *  count: starting point of second loop
 *
 */

void more_numbers(void)
{
	int start  = 0;

	while (start < 10)
	{
		int count = 0;

		while (count < 14)
		{
			_putchar(count + '0');
			count++;
		}
	_putchar('\n');
	start++;
	}
}
